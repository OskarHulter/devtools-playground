{
  "$schema": "https://json.schemastore.org/package",
  "name": "devtools-playground",
  "version": "0.1.0",
  "private": true,
  "author": "Oskar Hulter <oskarhulter@gmail.com> (https://oskarhulter.com)",
  "description": "My short description of my awesome package",
  "license": "MIT",
  "main": "./src/main.js",
  "types": "./lib/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OHulter/devtools-playground.git"
  },
  "bugs": {
    "url": "https://github.com/OHulter/devtools-playground/issues"
  },
  "homepage": "https://github.com/OHulter/devtools-playground/package.json#readme",
  "packageManager": "yarn@4.3.1",
  "engines": {
    "node": "v20.11.1"
  },
  "keywords": [
    "short",
    "relevant",
    "keywords",
    "for",
    "searching"
  ],
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 500,
    "showDetails": true
  },
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "bump": "yarn dlx beachball bump",
    "change-check": "yarn dlx beachball check",
    "tpc": "yarn dlx beachball check -r https://registry.npmjs.org -m 'fix: applying package updates'",
    "tpb": "yarn dlx beachball bump -r https://registry.npmjs.org -m 'fix: applying package updates'",
    "p": "yarn dlx beachball publish -r https://registry.npmjs.org -m 'fix: applying package updates' --no-publish",
    "postbuild": "yarn 404",
    "dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true node server.js",
    "serve": "http-server out -p 8080",
    "downloadLocales": "locize download --project-id=0842ada9-1d1d-4d48-ab27-08f6a132f558 --ver=latest --clean=true --path=./public/locales",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rm -rf .yarn/cache | exit 0 && rimraf .next out | exit 0",
    "clean-deps": "rm -rf .yarn/cache | exit 0 && rimraf .next out | exit 0 && rm -rf node_modules | exit 0",
    "nuke:install": "rimraf ./node_modules ./package-lock.json",
    "toc": "i18next-resources-for-ts toc -i ./public/locales/en -o ./@types/resources.ts",
    "merge": "i18next-resources-for-ts merge -i ./public/locales/en -o ./@types/resources.json",
    "interface": "i18next-resources-for-ts interface -i ./public/locales/en -o ./@types/resources.d.ts",
    "dev-next": "./node_modules/.bin/dotenvx run -- next dev",
    "build": "./node_modules/.bin/dotenvx run -- next build",
    "start": "./node_modules/.bin/dotenvx run -- next start",
    "lint": "./node_modules/.bin/dotenvx run --next lint",
    "ui:dev": "ladle serve",
    "ui:build": "ladle build",
    "ui:preview": "ladle preview",
    "analyze": "./node_modules/.bin/dotenvx run -- ANALYZE=true next build",
    "analyze:server": "./node_modules/.bin/dotenvx run -- BUNDLE_ANALYZE=server next build",
    "analyze:browser": "./node_modules/.bin/dotenvx run -- BUNDLE_ANALYZE=browser next build",
    "dev-https": "./node_modules/.bin/dotenvx run -- NODE_TLS_REJECT_UNAUTHORIZED=0 next dev --experimental-https",
    "dx": "./node_modules/.bin/dotenvx run -- yarn dev",
    "test-codegen": "./node_modules/.bin/dotenvx run -- yarn playwright codegen http://localhost:3000",
    "type-check": "./node_modules/.bin/dotenvx run -- tsc --pretty --noEmit",
    "type-check:ci": "./node_modules/.bin/dotenvx run -- tsc-absolute --pretty --noEmit",
    "lint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
    "lint:report": "eslint . --format json --output-file ../../lint-results/web.json",
    "check-changed-files": "ts-node scripts/ts-check-changed-files.ts",
    "translate-locales": "ts-node scripts/check-missing-translations.ts",
    "dep-check": "yarn dlx qnm doctor --sort duplicates",
    "eda-dev": "cd event-architecture-catalog && npm run dev",
    "eda-build": "cd event-architecture-catalog && npm run build && cd ..",
    "eda-start": "cd event-architecture-catalog && npm run start"
  },
